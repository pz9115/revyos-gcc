# Only XLEN and ABI affect Linux multilib dir names, e.g. /lib32/ilp32d/

MULTILIB_DIRNAMES := \
	$(patsubst rv32%,lib32,\
		$(patsubst rv32%xtheadc,lib32xthead,\
			$(patsubst rv32imafdcv_zfh_xtheadc,lib32v_xthead,\
	$(patsubst rv64%,lib64,\
		$(patsubst rv64%xtheadc,lib64xthead,\
			$(patsubst rv64imafdcv_zfh_xtheadc,lib64v_xthead,\
				$(patsubst rv64%zvlsseg0p7_xtheadc,lib64v0p7_xthead,$(MULTILIB_DIRNAMES))))))))

MULTILIB_OSDIRNAMES := $(patsubst lib%,../lib%,$(MULTILIB_DIRNAMES))
